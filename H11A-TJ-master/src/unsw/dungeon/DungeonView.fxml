<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.Pane?>

<Pane fx:id="pane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="#handleKeyPress" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar fx:id="bar">
        <menus>
          <Menu mnemonicParsing="false" text="Options">
            <items>
                  <MenuItem fx:id="restart" mnemonicParsing="false" onAction="#handleRestart" text="Restart" />
                  <MenuItem fx:id="PauseContinue" mnemonicParsing="false" onAction="#handlePauseContinue" text="Pause" />
                  <MenuItem fx:id="BackMenu" mnemonicParsing="false" onAction="#handleBackToMenu" text="Back To Menu" />
                  <MenuItem fx:id="Close" mnemonicParsing="false" onAction="#handleExit" text="Close" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</Pane>
